<div class="fields">
  <%= f.label :title, "Question" %>
  <%= f.text_field :title %>
  <%= f.hidden_field :_destroy %>
  <%= remove_link "remove", "question" %>
  <p>
  <%= f.fields_for :search_items do |builder| %>
    <%= render 'search_item_fields', f: builder %>
  <% end %>
  </p>
  <p><%= link_to_add_fields "Add Item", f, :search_items %></p>
</div>
